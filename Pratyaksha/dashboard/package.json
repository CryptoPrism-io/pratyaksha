{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "tsc -b && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "node dist/server/index.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:desktop": "playwright test --project=desktop-chrome --project=desktop-firefox --project=desktop-safari",
    "test:ipad": "playwright test --project=ipad-pro-landscape --project=ipad-pro-portrait --project=ipad-mini",
    "test:mobile": "playwright test --project=mobile-iphone-14-pro-max --project=mobile-iphone-14 --project=mobile-iphone-se --project=mobile-pixel-7 --project=mobile-galaxy-s23",
    "test:report": "playwright show-report test-results/html-report"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.16",
    "@tremor/react": "^3.18.7",
    "@types/d3": "^7.4.3",
    "@types/d3-sankey": "^0.12.5",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "driver.js": "^1.4.0",
    "express": "^4.21.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^13.6.0",
    "groq-sdk": "^0.37.0",
    "lucide-react": "^0.562.0",
    "luxon": "^3.7.2",
    "multer": "^2.0.2",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.11.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
